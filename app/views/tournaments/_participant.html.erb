    </th>
	<th>
	<%= form.text_field :placement, :size => 15 %>
	</th>
	<th>
    <%= form.text_field :number_of_rebuys, :size => 15 %>
    </th>
	<th>
	<%= form.text_field :number_of_addons, :size => 15 %>
